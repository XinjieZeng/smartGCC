                        <GridPane alignment="center" hgap="10" prefHeight="200" prefWidth="320" vgap="10">

                            <Text text="Debugging Options" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />

                            <RadioButton mnemonicParsing="false" text="debug -g " GridPane.columnIndex="1" GridPane.rowIndex="1">
                                <toggleGroup>
                                    <ToggleGroup fx:id="debugToggleGroup" />
                                </toggleGroup>
                            </RadioButton>

                            <RadioButton mnemonicParsing="false" text="debug -gdbb " toggleGroup="$debugToggleGroup" GridPane.columnIndex="1" GridPane.rowIndex="2" />
