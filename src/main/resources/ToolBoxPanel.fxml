                                    <ToggleGroup fx:id="compileToggleGroup" />
                                </toggleGroup>
                            </RadioButton>

                            <RadioButton mnemonicParsing="false" text="Compile to object File" toggleGroup="$compileToggleGroup" GridPane.columnIndex="1" GridPane.rowIndex="2" />
