                            <Text text="Code Optimization Options" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />

                            <RadioButton mnemonicParsing="false" text="optimization level 1" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                <toggleGroup>
                                    <ToggleGroup fx:id="optimizeToggleGroup" />
